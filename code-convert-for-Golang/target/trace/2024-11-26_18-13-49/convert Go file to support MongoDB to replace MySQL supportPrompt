	deviceSweeperDataRowsExpectAutoSet   = strings.Join(stringx.Remove(deviceSweeperDataFieldNames, "`id`", "`create_at`", "`create_time`", "`created_at`", "`update_at`", "`update_time`", "`updated_at`"), ",")
